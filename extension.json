{
  "name": "MCBBSWikiUtils",
  "author": "Litwak913",
  "url": "https://github.com/mcbbs-wiki/MW-Utils",
  "description": "MCBBS Wiki Utils",
  "version": "1.0",
  "requires": {
    "MediaWiki": ">= 1.31.0"
  },
  "license-name": "GPL-2.0-or-later",
  "type": "other",
  "manifest_version": 1,
  "AutoloadNamespaces": {
    "MCBBSWiki\\": "includes/"
  },
  "Hooks": {
    "SkinAddFooterLinks": "MCBBSWiki\\Hooks::customFooter",
    "ParserFirstCallInit": "MCBBSWiki\\Hooks::onParserFirstCallInit"
  },
  "MessagesDirs": {
    "MCBBSWikiUtils": ["i18n"]
  },
  "ResourceModules": {
    "ext.mcbbswikiutils.pagealert": {
      "localBasePath": "resources/ext.mcbbswikiutils.pagealert",
      "remoteExtPath": "MCBBSWikiUtils/resources/ext.mcbbswikiutils.pagealert",
      "packageFiles": ["page-alert.js"],
      "dependencies": ["oojs-ui-core", "oojs-ui-windows"],
      "messages": ["ok"]
    }
  }
}
